\subsection{Aufgabe 5.6(H)}

\subsubsection{(a)}
\begin{proof}
$ $\newline

z.z. $\frac{ad}{bd}=\frac{a}{b}$
\begin{align*}
\frac{ad}{bd}
&=(ad)(bd)^{-1}\\
&=(ad)(b^{-1}d^{-1})\\
&=a(d(b^{-1}d^{-1}))\\
&=a(d(d^{-1}b^{-1}))\\
&=a((dd^{-1})b^{-1})\\
&=ab^{-1}\\
&=\frac{a}{b}
\end{align*}
\end{proof}

\subsubsection{(b)}
\begin{proof}
$ $\newline

z.z. $\frac{a}{b}+\frac{c}{d}=\frac{ad+bc}{bd}$
\begin{align*}
\frac{ad+bc}{bd}
&=(ad+bc)(bd)^{-1}\\
&=(ad)(bd)^{-1}+(bc)(bd)^{-1}\\
&=(ad)(b^{-1}d^{-1})+(bc)(b^{-1}d^{-1})\\
&=(ad)(d^{-1}b^{-1})+(cb)(b^{-1}d^{-1})\\
&=a(d(d^{-1}b^{-1}))+c(b(b^{-1}d^{-1}))\\
&=a((dd^{-1})b^{-1})+c((bb^{-1})d^{-1})\\
&=ab^{-1}+cd^{-1}\\
&=\frac{a}{b}+\frac{c}{d}
\end{align*}
\end{proof}

